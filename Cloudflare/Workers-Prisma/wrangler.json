{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "workers-prisma",
  "main": "src/index.ts",
  "compatibility_date": "2025-01-29",
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "workers-prisma",
      "database_id": "YOUR_DATABASE_ID"
    }
  ]
}
